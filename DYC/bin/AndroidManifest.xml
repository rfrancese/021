<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dyc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/dyc_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.dyc.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dyc.Food.FoodActivity"
            android:label="@string/title_activity_second" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dyc.VolleyTest"
            android:label="@string/title_activity_volley_test" >
        </activity>
        <activity
            android:name="com.example.dyc.Sleep.SleepActivity"
            android:label="@string/title_activity_sleep" >
        </activity>
        <activity
            android:name="com.example.dyc.News.NewsActivity"
            android:label="@string/title_activity_news" >
        </activity>
        <activity
            android:name="com.example.dyc.Photogallery.PhotogalleryActivity"
            android:label="@string/title_activity_photogallery" >
        </activity>
    </application>

</manifest>
